CREATE TABLE Project (project_id INTEGER PRIMARY KEY, project_name VARCHAR(200) NOT NULL, project_description TEXT, project_status VARCHAR(50));


// Mobile table create command:

CREATE TABLE public.mobile (model_number INTEGER PRIMARY KEY, model_name VARCHAR(100) NOT NULL, model_color VARCHAR(50), sim_type VARCHAR(50), network_type VARCHAR(20), battery_capacity INTEGER, internal_storage INTEGER, ram VARCHAR(50), processor_type VARCHAR(100));

